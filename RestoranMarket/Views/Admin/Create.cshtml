@model RegisterModel

@{
    Layout = "_AdminLayout";
}




<div class="bg-primary text-white p-1 m-1">
    <h2 class="p-2">Kullanıcı Listesi</h2>
</div>

<form class="m-1 p-1" asp-action="Create" method="post">

    <div class="text-danger" asp-validation-summary="All"></div>

    <div class="form-group">
        <label asp-for="UserName">
            KULLANICI ADI
        </label>
        <input asp-for="UserName" class="form-control" />
    </div>

    <div class="form-group">
        <label asp-for="Password">
            PAROLA
        </label>
        <input asp-for="Password" class="form-control" />
    </div>

    <div class="form-group">
        <label asp-for="Email">
            EMAİL
        </label>
        <input asp-for="Email" class="form-control" />
    </div>
    <input type="submit" class="btn btn-primary btn-lg" value="Kaydet" />
    <button asp-action="Index" class="btn btn-danger btn-lg">İptal</button>
</form>

